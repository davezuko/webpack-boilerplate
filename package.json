{
  "name": "webpack-boilerplate",
  "version": "0.0.0",
  "description": "Webpack config builder boilerplate.",
  "main": "index.js",
  "scripts": {
    "compile": "webpack",
    "compile:prod": "NODE_ENV=production webpack",
    "dev": "webpack-dev-server --port 3000 --hot --inline --colors",
    "dev:quiet": "webpack-dev-server --port 3000 --hot --inline --colors --no-info --quiet",
    "test": "node ./node_modules/karma/bin/karma start"
  },
  "author": "David Zukowski <david@zuko.me> (http://zuko.me)",
  "license": "MIT",
  "devDependencies": {
    "autoprefixer-loader": "^1.2.0",
    "babel": "^5.2.15",
    "babel-core": "^5.2.15",
    "babel-loader": "^5.0.0",
    "chai": "^2.3.0",
    "css-loader": "^0.12.0",
    "extract-text-webpack-plugin": "^0.8.0",
    "html-webpack-plugin": "^1.3.0",
    "jshint": "^2.7.0",
    "jshint-loader": "^0.8.3",
    "jshint-loader-stylish": "^1.0.0",
    "karma": "^0.12.32",
    "karma-chai": "^0.1.0",
    "karma-chai-plugins": "^0.5.0",
    "karma-coverage": "^0.3.1",
    "karma-mocha": "^0.1.10",
    "karma-phantomjs-launcher": "^0.1.4",
    "karma-spec-reporter": "0.0.19",
    "karma-webpack": "^1.5.1",
    "mocha": "^2.2.5",
    "node-sass": "^3.0.0",
    "phantomjs-polyfill": "0.0.1",
    "sass-loader": "^1.0.2",
    "style-loader": "^0.12.1",
    "webpack": "^1.9.0",
    "webpack-dev-server": "^1.8.2",
    "yargs": "^3.9.1"
  }
}
